{
  "name": "rails-factorybot-nav",
  "displayName": "Rails FactoryBot Navigator",
  "description": "Navigate to FactoryBot factory definitions with Ctrl+Click",
  "version": "0.0.2",
  "publisher": "faktus",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Faktus-eu/rails-factorybot-nav"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Rails FactoryBot Navigator",
      "properties": {
        "railsFactoryBotNav.customFactoryPath": {
          "type": "string",
          "default": "spec/factories",
          "description": "Custom path to FactoryBot factories relative to project root"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish-minor": "vsce publish minor"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  }
}
